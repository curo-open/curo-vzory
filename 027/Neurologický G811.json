        },
        "value": {
          "z": {
            "value": "avostran. spastic. hemipar. po NCMP hemor. <br>kontrakt. LDK spastic.  a LHK<br>sek.  VAS CThLS<br>artrosis LBK II<br>SI algia a posun sek. podmien. <br>zníž.  sebeobsluha<br>",
            "props": {}
          }
        }
